<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN"
        "http://castor.org/mapping.dtd">
<mapping>
    <class name="com.siwoo.projpa.support.jaxb.Sqlmap">
        <map-to xml="sqlmap">
            <field name="sql" type="com.siwoo.projpa.support.jaxb.SqlType" required="true" collection="arraylist">
                <bind-xml name="sql" node="element" />
            </field>
        </map-to>
    </class>
    <class name="com.siwoo.projpa.support.jaxb.SqlType">
        <map-to xml="sql">
            <field name="key" type="string" required="true">
                <bind-xml name="key" node="attribute" />
            </field>
            <field name="value" type="string" required="true">
                <bind-xml node="text" />
            </field>
        </map-to>
    </class>
</mapping>